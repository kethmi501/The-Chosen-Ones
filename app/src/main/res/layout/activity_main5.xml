<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity5">

    <EditText
        android:id="@+id/lbl_noMealscurrent"
        android:layout_width="396dp"
        android:layout_height="52dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/lbl_noMealsCurrent"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="168dp" />

    <EditText
        android:id="@+id/lbl_noMealsedit"
        android:layout_width="396dp"
        android:layout_height="52dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/lbl_noMealsedit"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="235dp" />

    <Button
        android:id="@+id/btn_noMeals2"
        android:layout_width="42dp"
        android:layout_height="44dp"
        android:text="@string/btn_noMeals"
        android:textColor="#212121"
        app:backgroundTint="#2196F3"
        tools:layout_editor_absoluteX="353dp"
        tools:layout_editor_absoluteY="235dp" />

    <EditText
        android:id="@+id/lbl_eCaloryedit"
        android:layout_width="395dp"
        android:layout_height="59dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/lbl_eCaloryedit"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="509dp" />

    <TextView
        android:id="@+id/lbl_navbar4"
        android:layout_width="411dp"
        android:layout_height="60dp"
        android:background="#0095B6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btn_back3"
        android:layout_width="65dp"
        android:layout_height="56dp"
        android:background="#0095B6"
        app:layout_constraintBottom_toBottomOf="@+id/lbl_navbar"
        app:layout_constraintStart_toStartOf="@+id/lbl_navbar"
        app:layout_constraintTop_toTopOf="@+id/lbl_navbar"
        app:srcCompat="?attr/actionModeCloseDrawable" />

    <TextView
        android:id="@+id/lbl_edit"
        android:layout_width="146dp"
        android:layout_height="42dp"
        android:layout_marginTop="12dp"
        android:text="@string/lbl_edit"
        android:textColor="#FBFAFA"
        android:textSize="24sp"
        android:textStyle="normal|bold"
        app:layout_constraintBottom_toBottomOf="@+id/lbl_navbar"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="76dp" />

    <TextView
        android:id="@+id/text_nameedit"
        android:layout_width="395dp"
        android:layout_height="49dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="107dp" />

    <TextView
        android:id="@+id/text_size2"
        android:layout_width="118dp"
        android:layout_height="50dp"
        tools:layout_editor_absoluteX="285dp"
        tools:layout_editor_absoluteY="170dp" />

    <View
        android:id="@+id/imageviewedit"
        android:layout_width="186dp"
        android:layout_height="142dp"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="321dp" />

    <EditText
        android:id="@+id/text_caloryEdit"
        android:layout_width="118dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="285dp"
        tools:layout_editor_absoluteY="518dp" />

    <Button
        android:id="@+id/btn_saveedit"
        android:layout_width="139dp"
        android:layout_height="53dp"
        android:layout_marginBottom="64dp"
        android:text="@string/btn_saveedit"
        android:textColor="#0B0B0B"
        android:textSize="20sp"
        app:backgroundTint="#009688"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbl_eCalory"
        app:layout_constraintVertical_bias="0.292" />
</androidx.constraintlayout.widget.ConstraintLayout>