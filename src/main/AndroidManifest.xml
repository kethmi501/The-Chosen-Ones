<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginwithsignup">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginWithSignUp">
        <activity android:name=".DesitinationActivity"></activity>
        <activity android:name=".AlarmMainActivity" />
        <activity android:name=".first" />
        <activity android:name=".Forms.HomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Forms.SignupActivity" />

        <receiver android:name=".AlarmReceiver" />
    </application>

</manifest>